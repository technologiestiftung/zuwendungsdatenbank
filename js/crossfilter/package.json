{
  "name": "crossfilter2",
  "version": "1.4.3",
  "description": "Fast multidimensional filtering for coordinated views.",
  "license": "Apache-2.0",
  "keywords": [
    "analytics",
    "visualization",
    "crossfilter"
  ],
  "author": {
    "name": "Mike Bostock",
    "url": "http://bost.ocks.org/mike"
  },
  "contributors": [
    {
      "name": "Jason Davies",
      "url": "http://www.jasondavies.com/"
    }
  ],
  "maintainers": [
    {
      "name": "Gordon Woodhull",
      "url": "https://github.com/gordonwoodhull"
    },
    {
      "name": "Tanner Linsley",
      "url": "https://github.com/tannerlinsley"
    },
    {
      "name": "Ethan Jewett",
      "url": "https://github.com/esjewett"
    }
  ],
  "homepage": "http://crossfilter.github.com/crossfilter/",
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/crossfilter/crossfilter.git"
  },
  "dependencies": {
    "lodash.result": "^4.4.0"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "d3": "~5.8.0",
    "eslint": "5.12.1",
    "package-json-versionify": "1.0.4",
    "semver": "^5.3.0",
    "uglify-js": "3.4.9",
    "vows": "0.8.2"
  },
  "scripts": {
    "benchmark": "node test/benchmark.js",
    "build": "browserify index.js -t package-json-versionify --standalone crossfilter -o crossfilter.js && uglifyjs --compress --mangle --screw-ie8 crossfilter.js -o crossfilter.min.js",
    "clean": "rm -f crossfilter.js crossfilter.min.js",
    "test": "./node_modules/.bin/vows --verbose && eslint src/"
  },
  "files": [
    "src",
    "index.js",
    "crossfilter.js",
    "crossfilter.min.js"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "Uint8Array": true,
      "Uint16Array": true,
      "Uint32Array": true
    },
    "extends": "eslint:recommended"
  }
}
